public class TechNumber
{
   public static void main(String args[])
   {
       int i;
       for(i=1000;i<=9999;i++)
       {
           int firsthalf = i % 100;
           int secondhalf = i / 100;
           int sum = firsthalf + secondhalf;
           if(i == sum*sum)
           {
               System.out.print(i + " ");
           }
       }
   }
}
